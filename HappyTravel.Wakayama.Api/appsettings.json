{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2AndHttp3"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Database": {
    "ConnectionString": "Server={0};Port={1};Database=wakayama;Userid={2};Password={3};",
    "Options": "wakayama/connection-string"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "wakayama",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Telemetry": {
    "IsEnabled": false
  },
  "Elasticsearch": {
    "Indexes" : "wakayama/elastic/indexes",
    "ClientSettings": "wakayama/elastic/client-settings"
  }
}
