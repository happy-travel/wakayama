{
  "Elasticsearch": {
    "Indexes" : "wakayama-updater/elastic/indexes",
    "ClientSettings": "wakayama-updater/elastic/client-settings"
  },
  "LaunchOptions":
  {
    "LaunchMode": "UPDATER_LAUNCH_MODE"
  },
  "Updaters": {
    "Photon": {
      "Settings": "wakayama-updater/data-updaters/photon/settings"
    }
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "wakayama-updater",
    "Token": "HTDC_VAULT_TOKEN"
  }
}